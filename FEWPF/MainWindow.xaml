<Window x:Class="FEWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FEWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="142,36,0,0" TextWrapping="Wrap" Text="customerName" VerticalAlignment="Top" Width="232" Name="AddCustomerName"/>
        <TextBox HorizontalAlignment="Left" Margin="142,59,0,0" TextWrapping="Wrap" Text="customerLastName" VerticalAlignment="Top" Width="232" Name="AddLastName"/>
        <TextBox HorizontalAlignment="Left" Margin="142,82,0,0" TextWrapping="Wrap" Text="customerCompanyName" VerticalAlignment="Top" Width="232" Name="AddCompanyName"/>
        <Button Content="Add Customer" HorizontalAlignment="Left" Margin="142,105,0,0" VerticalAlignment="Top" Click="Button_Click" Name="AddCustomerBtn"/>

    </Grid>
</Window>
